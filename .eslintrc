{
    "env": {
        "browser": true,
        "es6": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "quotes": [2, "single"],
        "no-use-before-define": [2, "nofunc"],
        "no-multiple-empty-lines": [2, {"max": 1}],
        "eqeqeq": [2, "smart"],
        "semi": [2, "always"]
    }
}